{
	"name": "ashleyhindle/fodor-graylog2",
	"installpath": "/tmp/fodor-graylog2/",
	"provisioner": "fodor/provisioner.sh",
	"text": {
		"complete": "User/Pass: admin/password - it may take a minute for the graylog service to start"
	},
	"links": [
		{
			"title": "Graylog Backend",
			"url": "http://{{DOMAIN}}:9000/"
		},
		{
			"title": "Set admin user and pass",
			"url": "http://docs.graylog.org/en/1.3/pages/installation/graylog_ctl.html"
		},
		{
			"title": "Sending data",
			"url": "http://docs.graylog.org/en/1.3/pages/sending_data.html"
		}
	],
	"size": {
		"suggested": "4gb",
		"required": "2gb"
	},
	"distro": "ubuntu-14-04-x64",
	"description": "Automatically sets up a Graylog2 instance",
	"keywords": ["fodor", "graylog", "graylog2", "logstash"],
	"homepage": "https://github.com/ashleyhindle/fodor-graylog2",
	"inputs": [
		{
			"title": "Fake API Key",
			"name": "fakeapikey",
			"placeholder": "xxxx-xxxx-xxxx-xxxx",
			"type": "regex",
			"regex": "[a-zA-Z\\-0-9]+"
		}
	]
}
